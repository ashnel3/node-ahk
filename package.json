  "dependencies": {
    "ffi-napi": "^4.0.3",
    "ref-napi": "^3.0.3",
    "ref-wchar-napi": "^1.0.3"
  },
  "devDependencies": {
    "@types/ffi-napi": "^4.0.7",
    "@types/node": "^20.2.5",
    "@types/ref-napi": "^3.0.7",
